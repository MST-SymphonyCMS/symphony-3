<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <driver>mysql</driver>
  <character-set>utf8</character-set>
  <character-encoding>utf8</character-encoding>
  <runtime-character-set-alter>1</runtime-character-set-alter>
  <disable-query-caching>no</disable-query-caching>
  <host>%s</host>
  <port>%s</port>
  <user>%s</user>
  <password>%s</password>
  <db>%s</db>
  <table-name-prefix>%s</table-name-prefix>
</configuration>